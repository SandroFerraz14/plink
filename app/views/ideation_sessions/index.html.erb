<p id="notice"><%= notice %></p>

<h1>Listing Ideation Sessions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ideation_sessions.each do |ideation_session| %>
      <tr>
        <td><%= ideation_session.name %></td>
        <td><%= ideation_session.description %></td>
        <td><%= link_to 'Show', ideation_session %></td>
        <td><%= link_to 'Edit', edit_ideation_session_path(ideation_session) %></td>
        <td><%= link_to 'Destroy', ideation_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ideation session', new_ideation_session_path %>
